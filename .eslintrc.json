{
	"env": {
        "es6": true,
        "node": true,
        "jest": true
    },
	"extends": ["next/core-web-vitals", "eslint:recommended", "airbnb-base"],
	"rules": {
		"indent": [
			"error",
			4,
			{
				"SwitchCase": 1
			}
		],
		"max-len": 0,
		"import/extensions": 0,
		"implicit-arrow-linebreak": 0,
		"no-console": 0,
		"object-shorthand": 0,
		"import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
		"import/prefer-default-export": 0,
		"import/no-unresolved": 0
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"tsx": true
		}
	},
	"settings": {
		"import/resolver": {
			"typescript": {}
		}
	}
}